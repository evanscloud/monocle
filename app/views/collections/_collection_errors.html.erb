<% if @collection.errors.any? %>
  <div id="error_explanation">
    <div class="alert alert-danger alert-devise">
      <h5>Looks like there are problems:</h5>
      <% @collection.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
    </div>
  </div>
<% end %>
